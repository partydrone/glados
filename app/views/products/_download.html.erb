<%= link_to attachment_url(download, :file, filename: "#{download.title.parameterize(separator: '_')}"), class: "support-download-item row", download: attachment_url(download, :file, filename: "#{download.title.parameterize(separator: '_')}"), target: '_blank' do %>
  <div class="small-1 column">
    <%= image_tag('icons/document.svg') %>
  </div>
  <div class="small-11 columns">
    <%= download.title %>
  </div>
<% end if download.file %>

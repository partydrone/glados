<% cache redirect_rule do %>
  <%= content_tag_for :tr, redirect_rule do %>
    <td class="admin-left-edit">
      <%= render partial: 'options_edit', locals: {object: redirect_rule, instance_name: redirect_rule.source} %>
    </td>
    <td style="text-align: center">
      <%= form_for [:admin, redirect_rule], remote: true, authenticity_token: true do |f| %>
        <%= f.check_box :active, class: 'toggle-redirect-rule', style: 'margin-bottom: 0' %>
      <% end %>
    </td>
    <td>
      <%= redirect_rule.source %>
    </td>
    <td>
      <%= redirect_rule.destination %>
    </td>
    <td class="admin-right-delete">
      <%= render partial: 'options_delete', locals: {object: redirect_rule, instance_name: redirect_rule.source} %>
    </td>
  <% end %>
<% end %>

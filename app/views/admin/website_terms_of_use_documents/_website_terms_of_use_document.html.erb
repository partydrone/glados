<%= content_tag_for :tr, website_terms_of_use_document do %>
  <td class="admin-left-edit">
    <%= render partial: 'options_edit', locals: {object: website_terms_of_use_document, instance_name: website_terms_of_use_document.title} %>
  </td>
  <td>
    <%= link_to website_terms_of_use_document.title, website_terms_of_use_document %>
  </td>
  <td>
    <%= l website_terms_of_use_document.effective_on.to_date, format: :long %>
  </td>
  <td>
    <%= l website_terms_of_use_document.updated_at.to_date, format: :long %>
  </td>
  <td class="admin-right-delete">
    <%= render partial: 'options_delete', locals: {object: website_terms_of_use_document, instance_name: website_terms_of_use_document.title} %>
  </td>
<% end %>

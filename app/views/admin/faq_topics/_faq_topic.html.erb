<% cache faq_topic do %>
  <%= content_tag_for :tr, faq_topic do %>
  	<td class="admin-left-edit">
  		<%= render partial: 'options_edit', locals: {object: faq_topic, instance_name: faq_topic.title} %>
  	</td>
    <td>
      <%= faq_topic.title %>
    </td>
    <td class="admin-right-delete">
      <%= render partial: 'options_delete', locals: {object: faq_topic, instance_name: faq_topic.title} %>
    </td>
  <% end %>
<% end %>

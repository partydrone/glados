<% cache sales_terms_and_conditions_document do %>
  <%= content_tag_for :tr, sales_terms_and_conditions_document do %>
    <td>
      <%= link_to sales_terms_and_conditions_document.title, sales_terms_and_conditions_document %>
    </td>
    <td>
      <%= l sales_terms_and_conditions_document.effective_on.to_date, format: :long %>
    </td>
    <td>
      <%= l sales_terms_and_conditions_document.updated_at.to_date, format: :long %>
    </td>
    <td>
      <div class="admin-nav">
        <ul class="dropdown menu" data-dropdown-menu data-options="disableHover:true;clickOpen:true;">
          <li>
            <a href="#" class="ellipsis-link"><%= image_tag('icons/ellipsis.svg', class: 'small-icon') %></a>
            <ul class="menu">
              <li>
                <%= link_to edit_sales_terms_and_conditions_document_path(sales_terms_and_conditions_document), class: 'admin-edit', title: %(Edit "#{sales_terms_and_conditions_document.title}") do %>
                  Edit
                <% end %>
              </li>
              <li>
                <%= link_to sales_terms_and_conditions_document, class: 'delete admin-delete', data: { confirm: %(Are you sure you want to delete "#{sales_terms_and_conditions_document.title}"? This cannot be undone.) }, method: :delete, title: %(Delete "#{sales_terms_and_conditions_document.title}") do %>
                  Delete
                <% end %>
              </li>
            </ul>
          </li>
        </ul>
      </div>
    </td>
  <% end %>
<% end %>

<div class="tag-container medium-6 large-4 end columns">
	<div data-equalizer-watch>
		<%= link_to feature, class: 'tag-result-tile' do %>
			<div class="row">
				<div class="small-6 large-3 columns">
			  	<%= attachment_image_tag feature, :hero_image, :fill, 70, 40 %>
			  </div>
			  <div class="small-6 large-9 columns">
				  <h3><%= feature.title %></h3>
				  <p><%= feature.description %></p>
			  </div>
		  </div>
		<% end %>
	</div>
</div>
